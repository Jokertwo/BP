<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?><?import javafx.scene.control.*?><?import javafx.scene.layout.*?><?import javafx.scene.text.Font?>
<VBox fx:id="content" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="400.0" prefWidth="600.0"
      xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="my.bak.trafic.view.controler.PluginManagerControler">
    <children>
        <Label text="Plugins" VBox.vgrow="ALWAYS">
            <VBox.margin>
                <Insets bottom="20.0" left="10.0" right="20.0" top="20.0" />
            </VBox.margin>
            <font>
                <Font name="System Bold" size="17.0" />
            </font>
        </Label>
        <HBox prefHeight="333.0" prefWidth="567.0" VBox.vgrow="ALWAYS">
            <children>
                <ListView fx:id="pluginListView" prefHeight="400.0" prefWidth="443.0" HBox.hgrow="ALWAYS" />
                <VBox alignment="TOP_CENTER" prefHeight="324.0" prefWidth="170.0" spacing="5.0" HBox.hgrow="NEVER">
                    <children>
                        <Button fx:id="addButton" mnemonicParsing="false" onAction="#handleAddButton" prefHeight="31.0" prefWidth="135.0" text="Add" />
                        <AnchorPane prefHeight="325.0" prefWidth="150.0">
                            <VBox.margin>
                                <Insets top="75.0" />
                            </VBox.margin>
                            <children>
                                <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                      AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <children>
                                        <Label fx:id="staticInfo" alignment="TOP_LEFT" text="Info:" underline="true" visible="false">
                                            <font>
                                                <Font name="System Bold" size="15.0" />
                                            </font>
                                        </Label>
                                        <Label fx:id="infoLabel" alignment="TOP_LEFT" wrapText="true" />
                                    </children>
                                </VBox>
                            </children>
                        </AnchorPane>
                    </children>
                    <HBox.margin>
                        <Insets />
                    </HBox.margin>
                    <padding>
                        <Insets bottom="3.0" left="3.0" right="3.0" top="5.0" />
                    </padding>
                </VBox>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
</VBox>
